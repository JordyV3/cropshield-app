                            each categoria in categorias
                                option(value=categoria.id )= categoria.nombre

                div(class="sm:col-span-3")
                    label(for="cultivos" class="block text-sm font-medium leading-6 text-gray-900") Cultivos
                    div(class="mt-2")
                        select(id="cultivos" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6")
                            option(value="") - Seleccione -
                            each cultivo in cultivos
                                option(value=cultivo.id )= cultivo.nombre

            div(id="mapa-inicio" class="h-[600px] mt-5")

            section(class="py-5")
            div(class="border-t border-gray-200 pt-4")
                h2(class=" text-center sm:text-2xl text-lg font-semibold leading-7 text-gray-900") Analisis Nuevos

                div(class="grid grid-cols-1 lg:grid-cols-3 gap-5 mt-14")
                    each analisis in chile
                        include ./templates/card

            section(class="py-5")
                h2(class="text-center text-4xl font-extrabold pt-10") Preventivos

                div(class="grid grid-cols-1 lg:grid-cols-3 gap-5 mt-14")
                    each analisis in chile
                        include ./templates/card
    
