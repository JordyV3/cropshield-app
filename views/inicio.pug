                div(class="sm:col-span-3")
                    label(for="categorias" class="block text-sm font-medium leading-6 text-gray-900") Categorías
                    div(class="mt-2")
                        select(id="categorias" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6")
